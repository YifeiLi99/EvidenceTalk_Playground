{
  "type":"object",
  "required":["customer_type","lifestyle","family_status","income"],
  "properties":{
    "customer_type":{"type":"string","enum":["新购","换购","增购","售后服务","unknown"]},
    "lifestyle":{"type":"array","items":{"type":"string"}},
    "family_status":{
      "type":"object",
      "required":["marital","children"],
      "properties":{
        "marital":{"type":"string","enum":["single","married","unknown"]},
        "children":{"type":"string","enum":["has_children","none","unknown"]}
      }
    },
    "income":{
      "type":"object",
      "required":["value","confidence","evidence_turns"],
      "properties":{
        "value":{"type":"string"},
        "confidence":{"type":"number","minimum":0,"maximum":1},
        "evidence_turns":{"type":"array","items":{"type":"integer"}}
      }
    }
  }
}
